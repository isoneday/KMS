/*=================
    SIDE NAV
===================*/


.push_down{
    margin-top: -10px;
    border-top: 1px solid #DDDDDD;
}

.on_left{
    float: left;
    margin-right: 0 !important;
}

.on_right{
    margin-left: 0 !important;
    float: right;
}
.side_nav a.minimize{
    background-color: #000;
    display: none;
    height: 20px;
    width:26px;
    float: right;
    margin-right: 7px;
    background: black url('../images/graphics/open_close.png') no-repeat left;
}
.side_nav a.minimize:hover{
    background-position: left top;
}
.side_nav a.minimize_closed:hover{
    background-position: left bottom;
}

.side_nav a.minimize span, 
.side_nav a.minimize_closed span{
    display: none;
}

.side_nav.closed{
    width: 34px;
    width: 4.25%;
}

#main.min_side_nav{
    width: 886px;

}

.side_nav a.minimize span{display: none;}

.side_nav ul{
    margin: 0;
    list-style: none;	
    height:43px;
    /* background:#ccc; */

}
.side_nav li {
    margin-bottom:0px;
    border-bottom: 1px solid #ccc;

    /*	background:url("../images/graphics/sidebar_repeat.png") repeat-x scroll 0 0 transparent;*/



    /*  margin-top:10px; */
}
.side_nav ul ul li,
.side_nav ul ul ul {
    min-width: 250px;  /*Menu Width*/
    margin: 0;
    list-style: none;
    background:url("../images/graphics/white_carbon.png") repeat scroll 0 0 transparent;
    /*        -webkit-box-shadow: 2px 2px 5px 0px #cccccc;
            box-shadow: 2px 2px 5px 0px #cccccc; */

}

.side_nav ul li { 
    float: left; 
    position: relative;  
    width: 100%; 

}


.side_nav > ul > li ul,
.side_nav > ul > li ul ul,
.side_nav > ul > li ul ul ul {
    display: none;
    position: absolute;
    top: 0px;
    left: 100%;
    z-index: 9999;
    margin-top: 1px;
}
.side_nav > ul > li > ul{ margin-top: 0px;}

.side_nav > ul > li > ul > li:first-child > ul{
    margin-top: -1px;
}

.side_nav ul li ul li ul {
    margin-top: 0px;
}

.side_nav ul ul.slide_left,
.side_nav ul ul ul.slide_left,
.side_nav ul ul ul ul.slide_left{
    top: 0px; 
    left:auto;
    right:100%;
}

/* Root Menu */
.side_nav ul a {
    padding: 0 13px;/*0 10px;*/
    display: block;
    font: bold 12px sans-serif;
    text-decoration: none;
    /*	white-space: nowrap;*/
    line-height: 43px;
    color:#444;
    font-family:Helvetica,Arial,sans-serif;
    text-shadow: 1px 1px 0 #FFFFFF;
}
.side_nav ul a:hover {
    color:#fff;
    background-color:#999;
    text-shadow: none;

}
.side_nav ul a img{
    margin: 5px 5px -12px -3px;
    position: relative;
    top:-8px;}


.side_nav ul ul{
    z-index: 3;}
.side_nav ul ul ul{
    z-index: 4;}
.side_nav ul ul ul ul{
    z-index: 5;}
.side_nav ul ul ul ul ul{
    z-index: 6;}
.side_nav ul ul ul ul ul ul{
    z-index: 7;}
.side_nav ul ul ul ul ul ul ul{
    z-index: 7;}

/* Hover Function - Do Not Move */
.side_nav ul li:hover ul ul,
.side_nav ul li:hover ul ul ul,
.side_nav ul li:hover ul ul ul ul,
.side_nav ul li.iehover ul ul,
.side_nav ul li.iehover ul ul ul,
.side_nav ul li.iehover ul ul ul ul {
    display: none;

}

.side_nav ul li:hover ul,
.side_nav ul ul li:hover ul,
.side_nav ul ul ul li:hover ul,
.side_nav ul ul ul ul li:hover ul,
.side_nav ul li.iehover ul,
.side_nav ul ul li.iehover ul,
.side_nav ul ul ul li.iehover ul,
.side_nav ul ul ul ul li.iehover ul { 
    display: block;
    /*        overflow-y: auto;
            height: 350px;
            max-height: 400px;
            overflow-x:hidden;*/

}



/* Round corners
----------------------------------------------------------------------------------------------------*/
.submenu li{
    border-top: 1px solid #ccc;
    border-right: 1px solid #ccc;
    border-left: 1px solid #ccc;
    border-bottom: none;

}

.submenu{

    width: : 500px;
}


.side_nav ul li:first-child > a{
    -webkit-border-top-left-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topleft: 3px;
    -moz-border-radius-topright: 3px;
    /*	border-top-left-radius: 3px;
        border-top-right-radius: 3px;*/
    border: 0px!important;
}

.side_nav ul li:last-child > a{
    -webkit-border-bottom-right-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-radius-bottomright: 3px;
    -moz-border-radius-bottomleft: 3px;
    /*	border-bottom-right-radius: 3px;
        border-bottom-left-radius: 3px;*/
}

/*Accordion Menu*/
/* Accordion
----------------------------------------------------------------------------------------------------*/

li.openable{
    position: relative;
}
li.openable.active > a{
    color:#fff;
    text-shadow: none;
}

li.openable div.accordion{
    color:#666!important;
    /* 	border-top: 1px solid #fff; */
    font-weight: normal;
    display: none;
    background: none repeat scroll 0 0 #eee;


}

li.openable div.accordion a{
    background: url(../images/graphics/nav_bullet.png) 15px no-repeat;
    padding-left: 30px;
    font-weight: normal;
}

li div.accordion{display: none;}
li:target > div.accordion{
    display: block;
}

li.openable.active a{

    -webkit-border-radius: 0px !important;
    -moz-border-radius: 0px;
    border-radius: 0px;
}

ul.accordion > li.openable.active:first-child > a{	
    -webkit-border-top-left-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topleft: 3px;
    -moz-border-radius-topright: 3px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}
ul.accordion > li.openable:last-child div.accordion a:last-child,
ul.accordion > li.openable:last-child div.accordion {	
    -webkit-border-bottom-right-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-radius-bottomright: 3px;
    -moz-border-radius-bottomleft: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;

    border-bottom: none;
}

#side_nav ul li.openable div.accordion a{
    background: url(../images/graphics/nav_bullet.png) 20px no-repeat;
    padding-left: 37px;
    border-bottom: 1px solid #ddd;
}

#side_nav ul li ul li.openable div.accordion a{
    background: url(../images/graphics/nav_bullet.png) 6px no-repeat;
    padding-left: 23px;
}
