}
/* Thumbnail Tray
----------------------------*/
#thumb-tray {
    border-left: 1px solid #CCCCCC;
    height: 48px;
    left: 0;
    overflow: hidden;
    position: fixed;
    text-align: left;
    top: 57px;
    width: 100%;
    z-index: 3;
}
ul#thumb-list {
    border-left: 1px solid #CCCCCC;
    display: inline-block;
    left: 0;
    list-style: none outside none;
    margin-left: -1px;
    padding: 0;
    position: relative;
    width: 1336px !important;
}
ul#thumb-list li {
    border-right: 1px solid #DDDDDD;
    display: inline;
    float: left;
    height: 50px;
    list-style: none outside none;
    margin: 0;
    overflow: hidden;
    padding: 0;
}
ul#thumb-list li a {
    font-size:11.5px;
    text-transform:uppercase;
}
ul#thumb-list li:hover {
    cursor:pointer;
}
#thumb-prev, #thumb-next {
    bottom: 0;
    height: 50px;
    position: absolute;
    width: 30px;
    z-index: 5;
}
#thumb-prev {
    left:0;
    background-color:transparent;
    /*#f8f8f8;*/
    color:#fff!important;
    opacity:0.1;
}
#thumb-next {
    right:0;
    background-color:transparent;
    /*#f8f8f8;*/
    color:#fff!important;
    opacity:0.1;
}
#thumb-prev:hover {
    cursor:pointer;
    /*background: rgba(0,0,0,0.5);
    background-color:#E6E6E6;*/
    background: -moz-linear-gradient(left, rgba(248, 248, 248, 1) 0%, rgba(255, 255, 255, 0) 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%, rgba(248, 248, 248, 1)), color-stop(100%, rgba(255, 255, 255, 0)));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left, rgba(248, 248, 248, 1) 0%, rgba(255, 255, 255, 0) 100%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, rgba(248, 248, 248, 1) 0%, rgba(255, 255, 255, 0) 100%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(left, rgba(248, 248, 248, 1) 0%, rgba(255, 255, 255, 0) 100%);
    /* IE10+ */
    background: linear-gradient(to right, rgba(248, 248, 248, 1) 0%, rgba(255, 255, 255, 0) 100%);
    /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f8f8f8', endColorstr='#00ffffff', GradientType=1);
    /* IE6-9 */
    opacity:1;
}
#thumb-next:hover {
    cursor:pointer;
    /*background: rgba(0,0,0,0.5);
    background-color:#E6E6E6;*/
    background: -moz-linear-gradient(left, rgba(255, 255, 255, 0) 0%, rgba(248, 248, 248, 1) 100%);
    /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%, rgba(255, 255, 255, 0)), color-stop(100%, rgba(248, 248, 248, 1)));
    /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left, rgba(255, 255, 255, 0) 0%, rgba(248, 248, 248, 1) 100%);
    /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, rgba(255, 255, 255, 0) 0%, rgba(248, 248, 248, 1) 100%);
    /* Opera 11.10+ */
    background: -ms-linear-gradient(left, rgba(255, 255, 255, 0) 0%, rgba(248, 248, 248, 1) 100%);
    /* IE10+ */
    background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(248, 248, 248, 1) 100%);
    /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00ffffff', endColorstr='#f8f8f8', GradientType=1);
    /* IE6-9 */
    opacity:1;
}
