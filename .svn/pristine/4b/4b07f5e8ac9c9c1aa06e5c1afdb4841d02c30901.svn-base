/*Accordion Menu*/

/* Accordion
----------------------------------------------------------------------------------------------------*/
li.openable {
    position: relative;
}
li.openable.active > a {
    color:#fff;
    text-shadow: none;
}
li.openable div.accordion {
    color:#666!important;
    /*  border-top: 1px solid #fff; */
    font-weight: normal;
    display: none;
    background: none repeat scroll 0 0 #eee;
}
li.openable div.accordion a {
    background: url(../images/graphics/nav_bullet.png) 15px no-repeat;
    padding-left: 30px;
    font-weight: normal;
}
li div.accordion {
    display: none;
}
li:target > div.accordion {
    display: block;
}
li.openable.active a {
    -webkit-border-radius: 0px !important;
    -moz-border-radius: 0px;
    border-radius: 0px;
}
ul.accordion > li.openable.active:first-child > a {
    -webkit-border-top-left-radius: 3px;
    -webkit-border-top-right-radius: 3px;
    -moz-border-radius-topleft: 3px;
    -moz-border-radius-topright: 3px;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
}
ul.accordion > li.openable:last-child div.accordion a:last-child, ul.accordion > li.openable:last-child div.accordion {
    -webkit-border-bottom-right-radius: 3px;
    -webkit-border-bottom-left-radius: 3px;
    -moz-border-radius-bottomright: 3px;
    -moz-border-radius-bottomleft: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
    border-bottom: none;
}
#side_nav ul li.openable div.accordion a {
    background: url(../images/graphics/nav_bullet.png) 20px no-repeat;
    padding-left: 37px;
    border-bottom: 1px solid #ddd;
}
#side_nav ul li ul li.openable div.accordion a {
    background: url(../images/graphics/nav_bullet.png) 6px no-repeat;
    padding-left: 23px;
}
