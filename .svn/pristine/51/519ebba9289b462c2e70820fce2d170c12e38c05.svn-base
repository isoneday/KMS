$sql['get_pendaftar_beasiswa']="
SELECT 
	a.kmhsTransDaftBeaId 		AS idBea,
	b.kmhsBeaNama 				AS nmBea,
	c.kmhsSumBeaNama 			AS nmSumber,
	d.kmhsRefJnsBeaNama 		AS nmjenis,
