	f.refStatPdrId 				AS statID,
	f.refStatPdrNama 			AS stat,
	e.kmhsRefTipeDaftId 		AS tipeID,
	e.kmhsrefTipeDaftNama 		AS tipeDaftar,
	beasyaId
FROM 
	kemahasiswaan_trans_daftar_beasiswa a
LEFT JOIN
	kemahasiswaan_beasiswa b ON b.kmhsBeaId = a.kmhsTransDaftBeaId
LEFT JOIN
	kemahasiswaan_sumber_beasiswa c ON c.kmhsSumBeaId = b.kmhsRefSumBeaId
LEFT JOIN
	kemahasiswaan_ref_jenis_beasiswa d ON d.kmhsRefJnsBeaId = b.kmhsRefJnsBeaId
LEFT JOIN
	kemahasiswaan_ref_tipe_daftar e ON e.kmhsRefTipeDaftId = a.kmhsRefTipeDaftId
LEFT JOIN
	kemahasiswaan_ref_status_pendaftaran_beasiswa f ON f.refStatPdrId = a.kmhsTransDaftBeaStatPdrId
LEFT JOIN `beasiswa_syarat` 
    ON beasyaBeaId = a.kmhsTransDaftBeaId
WHERE 
	a.kmhsTransDaftBeaPdrNiu = '%s'
";

