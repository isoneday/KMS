    <script type="text/javascript">
        $(document).ready(function() {
            /*--- headline background ---*/
            var images = ['airport.jpg', 'distro.jpg', 'konser1.jpg', 'konser2.jpg', 'konser3.jpg', 'mall.jpg', 'restaurant.jpg', 'stasiun.jpg', 'store1.jpg'];
            $('.headline').css({'background-image': 'url(images/backdrop/' + images[Math.floor(Math.random() * images.length)] + ')'});

            /*--- <i> scroll function ---*/
            $('#scroll-to-next-section').click(function() {
                $("html, body").animate({scrollTop: 640}, 600);
                return false;
            });

            /*--- tab menu ---*/
            $('.features-menu > li > a[data-toggle="tab"]').hover(function() {
                $($(this).attr("href")).siblings().removeClass("in active");
                $(this).tab("show");
            });
            /*--- end: tab menu*/

            /*--- Fixed Navbar ---*/
            /*$(window).scroll(function() {
                // console.log($(window).scrollTop());
                if ($(window).scrollTop() > 100) {
                    $('#nav-fixed').css({"opacity": "1", "visibility": "visible"});
                }
                if ($(window).scrollTop() < 101) {
                    $('#nav-fixed').css({"opacity": "0", "visibility": "hidden"});
                }
                if ($(window).scrollTop() > 639) {
                    $('#scroll-to-next-section').parent().fadeOut();
                }
            });*/
            /*--- end: Fixed Navbar ---*/

            /*--- image detection ---*/
            $('.partner img').each(function() {
                if ($(this).width() > $(this).height()) {
                    $(this).css({'height': 'auto', 'width': '100%'});
                } else {
                    $(this).css({'height': '100%', 'width': 'auto'});
                }
            });

            /*--- Slider ---*/
            function option(other) {
                var option = {
                    dots: false,
                    center: true,
                    items: 5,
                    loop: true,
                    margin: 60,
                    autoplayTimeout: 2500,
                    autoplayHoverPause: true
                }
                var full = $.extend(option, other);
                // console.log(full);
                return full;
            }
            // $(".partner-carousel").owlCarousel(option(
            //   {autoplay : true}
            // ));
            /*--- end: Slider ---*/

            $(".testimoni-carousel").owlCarousel(option(
                    {
                        items: 1,
                        loop: true,
                        autoplay: true,
                        dots: true
                    }
            ));
        });
    </script>
