/* ======NEWS======= */
.news-wrapper{
    background:#fff;
    border-radius: 5px;
    padding: 15px;
    background: url(../images/news-bg.png) repeat;
    border-bottom: 4px double #ccc;
    border-left: 1px solid #ccc;
    border-top: 1px solid #ccc;
    border-right: 1px solid #CCC;
    height:205px;
    box-shadow: 0 1px 3px rgba(34, 25, 25, 0.1);
}
.info-wrapper{
    background:#fff;
    border-radius: 5px;
    padding: 15px;
    background: url(../images/news-bg.png) repeat;
    border-bottom: 4px double #ccc;
    border-left: 1px solid #ccc;
    border-top: 1px solid #ccc;
    border-right: 1px solid #CCC;
    height:100px;
    box-shadow: 0 1px 3px rgba(34, 25, 25, 0.1);
    margin-top:20px;
}
.news-tittle{
    font: 18px Arial, serif;
    border-bottom: 3px double #ddd;
    padding: 0 0 5px 0;
    margin-bottom:10px;
}
.tweet,
.query {
    font: 12px Arial, serif;
    color: #333;
    text-transform: capitalize;
}

.tweet_list {
    -webkit-border-radius: 0.5em;
    -moz-border-radius: 0.5em;
    border-radius: 0.5em;
    list-style: none;
    margin: 0;
    padding: 0;
    overflow-y: hidden;
    /* background-color: #8ADEE2; */
}

.tweet_list li {
    overflow-y: auto;
    overflow-x: hidden;
    padding: 0.5em;
    list-style-type: none;
    border-bottom: 1px dashed #CCCCCC;
    margin-bottom: 10px;

}

.tweet_list li a {
    color: #EA9307;
}


.tweet_list .tweet_even {
    /* background-color: #91E5E7; */
}

.tweet_list .tweet_avatar {
    padding-right: .5em; float: left;
}

.tweet_list .tweet_avatar img {
    vertical-align: middle;
}


/* ======FOOTER====== */


#footer {
    background: none repeat scroll 0 0 #000000;
    bottom: 0;
    color: #888888;
    font-family: Verdana,Arial,Helvetica,sans-serif;
    font-size: 10px;
    padding: 10px 0;
    position: fixed;
    text-align: center;
    text-transform: uppercase;
    width: 100%;
}

.dropdown-menu {
    z-index: 98 !important;
}

.table-edit {
    max-width: 100%;
    margin-bottom: 18px;
    width: 100%;
    background-color: #F5F5F5;
    border-collapse: collapse;
    border-color: #F9F9F9;
    border-style: solid;
    border-width: 1px;
    clear: both;
    font-family: Arial,sans-serif;
    /*font-size: 1.2em;*/
    margin: 0;
    padding: 0;
    text-align: left;

}
.table-edit tbody tr:nth-child(even) {
    -webkit-box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
    -moz-box-shadow:0 1px 0 rgba(255,255,255,.8) inset;
    box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
}
.table-edit th {
    background-color:#F9F9F9;
    border: 1px solid #eee;
    color: #4E4E4E;
    /*font-size: 0.85em;*/
    font-weight: normal;
    padding: 10px 10px 6px 15px;
    text-align: left;
    vertical-align: top; 
}
.table-edit .subhead th {
    background-color:#F9F9F9;
    color: #434343;
    font-weight: bold;

    padding: 5px 10px 5px 15px;
}
.table-edit td {
    background-color:#FEFEFE;
    border-top: 1px solid #ddd;
    color: #444444;
    padding: 4px 15px 4px 5px;
    vertical-align: top; 
}

.table-edit-popup {
    max-width: 100%;
    margin-bottom: 18px;
    width: 100%;
    clear: both;
    font-family: Arial,sans-serif;
    margin: 0;
    padding: 0;
    text-align: left;

}
.table-edit-popup tbody tr:nth-child(even) {
    -webkit-box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
    -moz-box-shadow:0 1px 0 rgba(255,255,255,.8) inset;
    box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
}
.table-edit-popup th {
    border-right: 1px dotted #DCDCDC;
    border-top: 1px dotted #DCDCDC;
    border-bottom: 1px dotted #DCDCDC;
    color: #4E4E4E;
    font-weight: normal;
    padding: 10px 10px 6px 15px;
    text-align: left;
    vertical-align: top; 
}
.table-edit-popup .subhead th {
    border-right: 0px;
    color: #5E5E5E;
    font-weight: bold;
    padding: 5px 10px 5px 15px;
    text-align:left;    
}
.table-edit-popup td {
    border-left: 1px dotted #DCDCDC;
    border-top: 1px dotted #DCDCDC;
    border-bottom: 1px dotted #DCDCDC;
    color: #444444;
    padding: 4px 15px 4px 5px;
    vertical-align: top; 
}

.table-edit-popup label {
    display: inline;
}

/* CLASSES FOR DATA DETAILS */

.table-details {
    background-color: #FEFEFE;
    clear: both;
    text-align: left;
    font-family: Arial, sans-serif;
    margin: 0 0 10px 0;
    width: 100%;
    border-collapse: collapse;
    border-top: 0px solid #ccc;
    border-left: 0px solid #ccc;
    border-right: 0px solid #ccc;
    border-bottom: 0px solid #ccc;
    position: relative; 
    padding:15px;
}
.table-details  th:first-child{
    border-radius:none;
}
.table-details:after, .table-details:before {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #FFFFFF;
    border-color: -moz-use-text-color #CCCCCC #CCCCCC;
    border-image: none;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    bottom: -2px;
    content: "";
    display: block;
    height: 1px;
    left: 3px;
    position: absolute;
    right: 3px;
}
.table-details:before {
    bottom: -4px;
    left: 6px;
    right: 6px;

}

.table-details tbody {

}
.table-details tr {
    border-left: 0px solid #F4CFCF;
    padding-left:10px;
}

.table-details .subhead th {
    /*background:#BFBFBF!important; */
    color:#5E5E5E;
    text-align:left;
    /*text-transform:uppercase;*/
    font-weight:bold!important;
}
.table-details th {
    color: #000;
    font-weight: normal;/*bold;*/
    padding: 6px 15px 6px 15px;
    vertical-align: top;
    border-top: 1px solid #EEEEEE;
    border-left: 1px solid #EEEEEE;
    border-right: 1px solid #EEEEEE;/*#F4CFCF;*/
    border-bottom: 1px solid #EEEEEE;/*#FCEAD6;*/
    background-color: #F9F9F9;/*rgba(244, 178, 102, 0.1);*/
}

.table-details td {
    color: #362F2D;
    padding: 4px 15px 4px 5px;
    border-top: 1px solid #EEEEEE;
    border-left: 1px solid #EEEEEE;/*#F4CFCF;*/
    border-right: 1px solid #EEEEEE;
    border-bottom: 1px solid #EEEEEE;
    /*text-indent: 10px;*/
    padding-left: 10px;

}

