        $this->mrTemplate->addVar('ipk', 'URL', GtfwDispt()->GetUrl('applicant.personal.data', (empty($inputAkademikShow['akademik_id'])?'add':'update').'Akademik', 'do', 'json'));

        /*===INPUT DATA BEASISWA===*/
        if (!empty($inputBeasiswa)) {
        }

        /*
        *  LAMPIRAN UPLOAD FILE
        */
