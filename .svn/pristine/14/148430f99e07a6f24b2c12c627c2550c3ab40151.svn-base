        }
        $this->mrTemplate->addVar('nonakademik', 'URL', GtfwDispt()->GetUrl('applicant.personal.data', (empty($inputPrestasiShow['prestasi_id'])?'add':'update').'Prestasi', 'do', 'json') . '&elmId=' . $elmId);

