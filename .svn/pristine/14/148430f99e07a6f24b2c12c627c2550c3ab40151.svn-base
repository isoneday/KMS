                $this->mrTemplate->AddVar('ipk', 'CUTI_NO', $cuti == 'N' ? 'checked' : '');
            } else {
                $this->mrTemplate->addVar('ipk', 'TITLE', 'Tambah Histori Akademik');
            }

            $smt = array(
                array('id'=>'GANJIL','semester'=>'GANJIL'),
                array('id'=>'GENAP','semester'=>'GENAP')
            );

            foreach ($smt as $key => $val) {
