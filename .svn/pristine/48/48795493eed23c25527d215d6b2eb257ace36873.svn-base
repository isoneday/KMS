    <script type="text/javascript">
        var body_height;
        // if(navigator.onLine){
        //     google.load('visualization', '1', {'packages': ['geochart']});
        // }
        $(function() {
            body_height = $('#gtfw_3_5_body').height();
        
            var $elem = $('.container3');
			
            $('#nav_up').fadeIn('slow');
            $('#nav_down').fadeIn('slow');  
			
            $(window).bind('scrollstart', function(){
                $('#nav_up,#nav_down').stop().animate({'opacity':'1'});
            });
            $(window).bind('scrollstop', function(){
                $('#nav_up,#nav_down').stop().animate({'opacity':'1'});
            });
			
            $('#nav_down').click(
            function (e) {
                $('html, body').animate({scrollTop: $elem.height()}, 1000);
            }
        );
            $('#nav_up').click(
            function (e) {
                $('html, body').animate({scrollTop: '0px'}, 1000);
            }
        );
            
            hideMenu();
            $('#slick-toggle').toggle(function(){hideMenu()}, function(){showMenu()});
 
            $('.header-btn2 > li > a').click(function(e){
                var isActive = $(this).hasClass('btn-active');
                if(isActive){
                    e.preventDefault();
                    return false;
                }
                else{
                    $('.header-btn2 > li > a').each(function(){
                        $(this).removeClass('btn-active');
                    });
                    $(this).addClass('btn-active');
                }
            });
            
            //$(".collapse").collapse();
            
            $('.notebox-done, .notebox-warning, .notebox-alert').alert();
        });
        
        function hideMenu(){
            cur = $('#slick-toggle');
            cur.addClass('slick-icon');

            $('#side_menu .clearfix').children('li').children('a').children('.menu-label').hide();
            $('.sidebar').animate({width:50}, 300);
            $(".container3").width('96%').animate({left:'50px'}, 300);
            
        }
        
        function showMenu(){
            cur = $('#slick-toggle');
            $('.sidebar').animate({width:220}, 300);
            $(".container3").width('83.5%').animate({left:'220px'},300, function(){              
                cur.removeClass('slick-icon');
                $('#side_menu .clearfix').children('li').children('a').children('.menu-label').show();
            });            
        }
               
    </script>
