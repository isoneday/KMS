        <script type='text/javascript'>
            $(document).ready(function(){
                $(".colorScale").fadeTo("fast", 0.9);
                $(".colorScale").hover(function(){
                    $(this).fadeTo("fast", 1.0);
                },function(){
                    $(this).fadeTo("fast", 0.9);
                });
    	
                // hides the slickbox as soon as the DOM is ready
                $('.sidebar').show();
                // shows the slickbox on clicking the noted link  
                $('#slick-show').click(function() {
                    $('.sidebar').show('slow');
                    return false;
                });
                // hides the slickbox on clicking the noted link  
                $('#slick-hide').click(function() {
                    $('.sidebar').hide('fast');
                    return false;
                });
              
                $('#slick-toggle').toggle(function() {
                    cur = $(this);
                    cur.addClass('slick-icon');
    
                    $('.sidebar').find('.menu-label').hide();
                    $('.sidebar').animate({width:50}, 300);
                    $(".container3").width('96%').animate({left:'54px'}, 300);
                },function(){
    
                    $('.sidebar').find('.menu-label').show();
                    $('.sidebar').animate({width:220}, 300);
                    $(".container3").width('83.5%').animate({left:'220px'},300, function(){
                      
                        cur.removeClass('slick-icon');
                    });
                });
            });
        </script>
