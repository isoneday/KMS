   <input type="hidden" name="aplikanwawancara_beasiswa_id" value="{ID_BEASISWA}">
   <table class="table-common">
	<thead>
		<tr>
			<th><!-- patTemplate:gtfwgetlang key="Document" / --></th>
			<th>{USER1}</th>
			<th>{USER2}</th>
			<th>{USER3}</th>
		</tr>
	</thead>
    <tbody>
		<!-- patTemplate:tmpl name="data" -->
		<tr class="{ROW_CLASS}">
			<td>{DESKRIPSI_WAWANCARA}</td>
			<td>{NILAI-1}</td>
			<td>{NILAI-2}</td>
			<td>{NILAI-3}</td>
		</tr>
		<!-- /patTemplate:tmpl -->
	</tbody>
    </table>
	
   </form>

</div>
